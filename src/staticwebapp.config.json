{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "routes": [
    {
      "route": "/calendar",
      "serve": "/calendar",
      "statusCode": 200
    },
    {
      "route": "/tasks",
      "serve": "/tasks",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/dashboard",
      "statusCode": 200
    }
  ]
}
